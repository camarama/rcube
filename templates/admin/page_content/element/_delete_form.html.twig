<form method="post" action="{{ path('admin_journal_comptable_element_delete', {'id': element.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ element.id) }}">
    <button class="btn">Delete</button>
</form>
